@model IEnumerable<Usuario>

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <link rel="stylesheet" href="css/estilo.css">
    <title>Index</title>
</head>

<body>
    @foreach (var usuarios in Model)
    {
        <tr>
            <td><a asp-route-id="@usuarios.Id">@usuarios.Email</a></td>
        </tr>
    }

</body>
</html>
